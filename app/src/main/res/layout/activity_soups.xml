<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Soups"
    android:background="@drawable/homescreen">

    <TextView
        android:id="@+id/veghead"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:text="Veg Soups"
        android:textSize="25dp"
        android:fontFamily="@font/chelsea_market"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="100dp"
        android:text="Name"
        android:fontFamily="@font/chelsea_market"
        android:textSize="18dp"
        android:textStyle="bold"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="100dp"
        android:layout_marginTop="100dp"
        android:text="Price"
        android:fontFamily="@font/chelsea_market"
        android:textSize="18dp"
        android:textStyle="bold"
        android:textColor="@color/white" />


    <TextView
        android:id="@+id/vso1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="140dp"
        android:text="Tomato Soup"
        android:textSize="18dp"
        android:fontFamily="@font/chelsea_market"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/vso2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="180dp"
        android:text="Corn Soup"
        android:fontFamily="@font/chelsea_market"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/vso1pr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="100dp"
        android:layout_marginTop="140dp"
        android:fontFamily="@font/chelsea_market"
        android:text="60"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/vso2pr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="100dp"
        android:layout_marginTop="180dp"
        android:fontFamily="@font/chelsea_market"
        android:text="80"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:textStyle="bold"/>


    <Button
        android:id="@+id/tom"
        android:layout_height="40dp"
        android:layout_width="60dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="132dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/chelsea_market"
        android:text="add"
        android:textSize="14dp" />

    <Button
        android:id="@+id/corn"
        android:layout_height="40dp"
        android:layout_width="60dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="172dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/chelsea_market"
        android:text="add"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/nonveghead"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="260dp"
        android:layout_centerHorizontal="true"
        android:text="Non Veg Soups"
        android:textSize="25dp"
        android:fontFamily="@font/chelsea_market"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/nvso1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="310dp"
        android:text="Chicken Soup"
        android:fontFamily="@font/chelsea_market"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/nvso2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="350dp"
        android:text="Mutton Soup"
        android:fontFamily="@font/chelsea_market"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/nvso1pr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="100dp"
        android:layout_marginTop="310dp"
        android:fontFamily="@font/chelsea_market"
        android:text="100"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/nvso2pr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="100dp"
        android:layout_marginTop="350dp"
        android:fontFamily="@font/chelsea_market"
        android:text="120"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/chi"
        android:layout_height="40dp"
        android:layout_width="60dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="302dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/chelsea_market"
        android:text="add"
        android:textSize="14dp"
        />

    <Button
        android:id="@+id/mut"
        android:layout_height="40dp"
        android:layout_width="60dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="342dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/chelsea_market"
        android:text="add"
        android:textSize="14dp" />

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_alignParentBottom="true"
    android:layout_marginBottom="50dp">

    <Button
        android:id="@+id/gotocart"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:fontFamily="@font/chelsea_market"
        android:textSize="16dp"
        android:text="open Cart" />

    <Button
        android:id="@+id/back"
        android:layout_width="150dp"
        android:textSize="16dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="40dp"
        android:fontFamily="@font/chelsea_market"
        android:text="go back" />
</LinearLayout>


</RelativeLayout>